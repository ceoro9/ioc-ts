{
  "name": "@ceoro9/ioc-ts",
  "version": "0.0.0-development",
  "description": "Inversion of control container on TypeScript",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceoro9/ioc-ts.git"
  },
  "scripts": {
    "compile": "tsc",
    "build": "rollup --config && npm run compile",
    "preinstall": "npx npm-force-resolutions",
    "start": "rollup -c -w",
    "release": "semantic-release",
    "clean": "rm -rf build/ coverage/ src/**/*.map",
    "lint": "eslint --ext ts,js,json .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "view:coverage": "serve coverage/lcov-report",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "ioc",
    "di",
    "javascript",
    "typescript",
    "node",
    "dependency injection",
    "dependency inversion",
    "inversion of control container"
  ],
  "author": "ceoro9",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ceoro9/ioc-ts/issues"
  },
  "homepage": "https://github.com/ceoro9/ioc-ts#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "npm-force-resolutions": "0.0.3",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "semantic-release": "^17.0.4",
    "semantic-release-cli": "^5.2.3",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "underscore.string": ">=3.3.5"
  },
  "resolutions": {
    "underscore.string": ">=3.3.5",
    "minimist": ">=1.2.2"
  }
}
